# =============================================================================
# .gitignore for Across Analytics Project
# =============================================================================

# -----------------------------------------------------------------------------
# Python
# -----------------------------------------------------------------------------
# Byte-compiled / optimized / DLL files
__pycache__/
*.py[cod]
*$py.class

# C extensions
*.so

# Distribution / packaging
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg

# PyInstaller
*.manifest
*.spec

# Installer logs
pip-log.txt
pip-delete-this-directory.txt

# Unit test / coverage reports
htmlcov/
.tox/
.nox/
.coverage
.coverage.*
.cache
nosetests.xml
coverage.xml
*.cover
*.py,cover
.hypothesis/
.pytest_cache/
pytest_cache/

# Virtual environments
across-analytics-env/
venv/
ENV/
env/
.venv/
.env.local

# dbt
target/
dbt_packages/

# mypy
.mypy_cache/
.dmypy.json
dmypy.json

# Pyre type checker
.pyre/

# pytype static type analyzer
.pytype/

# -----------------------------------------------------------------------------
# Environment Variables
# -----------------------------------------------------------------------------
# Keep your API keys and secrets safe - NEVER commit these!
.env
.env.local
.env.*.local
*.env

# -----------------------------------------------------------------------------
# Data Files
# -----------------------------------------------------------------------------
data/

# Raw data from APIs (can be re-fetched, often large)
data/raw/

# Processed data files (can be regenerated from raw data)
data/processed/

# Block-timestamp cache (auto-generated, can be re-fetched)
data/cache/

# temporary old files
temp_old_files/

# Project context
project_context/

# Temporary data files
*.tmp
*.temp

# Large data formats
*.parquet
*.jsonl
*.csv
!data/seeds/*.csv

# SQLite databases
*.db
*.sqlite
*.sqlite3

# -----------------------------------------------------------------------------
# IDE and Editor Settings
# -----------------------------------------------------------------------------
# Cursor IDE
.cursor/

#database files
*.db
*.sqlite
*.sqlite3


# Visual Studio Code
.vscode/
*.code-workspace

# PyCharm / JetBrains
.idea/
*.iml
*.ipr
*.iws

# Sublime Text
*.sublime-project
*.sublime-workspace

# Vim
*.swp
*.swo
*~

# -----------------------------------------------------------------------------
# OS Generated Files
# -----------------------------------------------------------------------------
# Windows
Thumbs.db
ehthumbs.db
Desktop.ini
$RECYCLE.BIN/
*.lnk

# macOS
.DS_Store
.AppleDouble
.LSOverride
._*
.Spotlight-V100
.Trashes

# Linux
*~

# -----------------------------------------------------------------------------
# Jupyter Notebooks
# -----------------------------------------------------------------------------
.ipynb_checkpoints/
*.ipynb_checkpoints

# -----------------------------------------------------------------------------
# Logs and Debugging
# -----------------------------------------------------------------------------
*.log
logs/
*.debug

# -----------------------------------------------------------------------------
# Miscellaneous
# -----------------------------------------------------------------------------
# Temporary files
*.bak
*.backup
*.old

# Archives
*.zip
*.tar.gz
*.rar
*.7z

